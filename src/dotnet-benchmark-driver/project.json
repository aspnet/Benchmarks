{
  "version": "1.0.0-*",
  "description": "Driver for ASP.NET Core Benchmarks",
  "repository": {
    "type": "git",
    "url": "git://github.com/AspNet/Benchmarks"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.DotNet.Cli.Utils": "1.0.0-*",
    "Microsoft.Extensions.CommandLineUtils": "1.0.0-*",
    "Microsoft.Net.Http.Client": "1.0.0-*",
    "Newtonsoft.Json": "8.0.3",
    "System.Runtime.Serialization.Primitives": "4.1.0-*",
    "Repository": "1.0.0-*",
    "Benchmarks.ClientJob": "1.0.0-*",
    "Benchmarks.ServerJob": "1.0.0-*",
    "System.Data.SqlClient": "4.0.0-*"
  },
  "frameworks": {
    "netstandardapp1.5": {
      "dependencies": {
        "NETStandard.Library": "1.5.0-*"
      },
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  }
}