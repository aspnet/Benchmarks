{
  "version": "1.1.0-*",
  "description": "Schedules benchmark jobs on both the server and client.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/AspNet/Benchmarks"
    }
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.DotNet.Cli.Utils": "1.0.0-*",
    "Microsoft.Extensions.CommandLineUtils": "1.1.0-*",
    "Repository": "1.1.0-*",
    "Benchmarks.ClientJob": "1.1.0-*",
    "Benchmarks.ServerJob": "1.1.0-*",
    "System.Data.SqlClient": "4.1.0",
    "System.Runtime.Serialization.Primitives": "4.1.1"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0-*",
          "type": "platform"
        }
      }
    }
  }
}