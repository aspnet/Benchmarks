    </modules>

    <security>

      <access sslFlags="None" />

      <authentication>
        <anonymousAuthentication enabled="true" userName="IUSR" />
      </authentication>

    </security>
    <aspNetCore processPath="" arguments="" stdoutLogEnabled="false" stdoutLogFile=".\stdout" />
  </system.webServer>

</configuration>
