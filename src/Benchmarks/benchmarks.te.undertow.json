{
  "Default": {
    "Source": {
      "Repository": "https://github.com/TechEmpower/FrameworkBenchmarks.git",
      "BranchOrCommit": "master",
      "DockerFile": "frameworks/Java/undertow/undertow.dockerfile",
      "DockerImageName": "undertow",
      "DockerContextDirectory": "frameworks/Java/undertow/"
    },
    "Port": 8080
  },
  "Plaintext-Undertow": {
    "Path": "/plaintext",
    "PresetHeaders": "Plaintext",
    "ScriptName": "pipeline",
    "PipelineDepth": 16
  },
  "PlaintextNonPipelined-Undertow": {
    "Path": "/plaintext",
    "PresetHeaders": "Plaintext"
  },
  "Json-Undertow": {
    "Path": "/json",
    "PresetHeaders": "Json"
  },
  "FortunesMySql-Undertow": {
    "Path": "/fortunes",
    "PresetHeaders": "Html",
    "Source": {
      "DockerFile": "frameworks/Java/undertow/undertow-mysql.dockerfile",
      "DockerImageName": "undertow_mysql"
    }
  },
  "FortunesPostgreSql-Undertow": {
    "Path": "/fortunes",
    "PresetHeaders": "Html",
    "Source": {
      "DockerFile": "frameworks/Java/undertow/undertow-postgresql.dockerfile",
      "DockerImageName": "undertow_postgresql"
    }
  },
  "FortunesMongoDb-Undertow": {
    "Path": "/fortunes",
    "PresetHeaders": "Html",
    "Source": {
      "DockerFile": "frameworks/Java/undertow/undertow-mongodb.dockerfile",
      "DockerImageName": "undertow_mongodb"
    }
  }
}
